		"react": "16.12.0"
	},
	"devDependencies": {
		"@types/node": "13.7.6",
		"@types/react": "16.9.22",
		"@types/rimraf": "2.0.3",
		"react": "16.12.0",
		"rimraf": "2.6.2",
		"ts-node": "8.6.2",
